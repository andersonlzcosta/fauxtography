<div class="container">
  <div class="form-wrapper" style="width: 400px; margin: 0 auto;">
    <h2>Select and Date and Time</h2>

    <%= simple_form_for [@photographer, @booking] do |f| %>
      <%= f.input :date, as: :string, required: false, input_html: {class: "datepicker"} %>

      <%= f.button :submit, "Book This Fauxtographer", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>




